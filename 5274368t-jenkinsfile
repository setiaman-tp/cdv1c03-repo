pipeline {
    stages {
        stage("clean") {
            steps {
                echo "Start Clean"
                bat "mvn clean"
            }
        }
        stage("test") {
            steps {
                echo "Start Test"
                bat "mvn test"
            }
        }
        stage("build") {
            steps {
                echo "Start Build"
                bat "mvn install -DskipTests"
            }
        }
 
    }
  
    post {
        always {
            echo "Pipeline completed"
        }
    }
}
